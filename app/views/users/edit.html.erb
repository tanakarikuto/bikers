  <div class="main user-edit">
    <div class="col-md-offset-4 col-md-4 edit-container">
      <div class="form-head">
        <div class=" text-center user-edit-form">
          <%= form_with model: @user, method: "patch", local: true do |f| %>
          <%= f.label :name %>
          <%= f.text_field :name, value: @user.name %>
          <%= f.label :email %>
          <%= f.text_field :email, value: @user.email %>
           <%= f.label :image_name %>
           <%= f.file_field :image_name %>
           <%= f.submit "更新" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
